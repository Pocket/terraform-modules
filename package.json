{
  "name": "@pocket/terraform-modules",
  "version": "0.0.0-development",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MPL-2.0",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build:dev": "npm run build && NODE_ENV=development npm run synth",
    "synth": "cdktf synth",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test-ci": "npm test -- --exit",
    "test:watch": "npm test -- --watch --watch-extensions ts -R min --watch-files src",
    "test": "jest",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next",
    "lint-check": "eslint --fix-dry-run \"src/**/*.ts\"",
    "lint-fix": "eslint --fix \"src/**/*.ts\"",
    "semantic-release": "semantic-release"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "echo ${nextRelease.version} > .version"
        }
      ]
    ]
  },
  "engines": {
    "node": ">=10.12"
  },
  "peerDependencies": {
    "@cdktf/provider-archive": "^0.0.3",
    "@cdktf/provider-aws": "^2.0.9",
    "@cdktf/provider-local": "^0.0.3",
    "@cdktf/provider-null": "^0.2.3",
    "@cdktf/provider-pagerduty": "^0.0.3",
    "cdktf": "^0.5.0",
    "constructs": "^3.3.147"
  },
  "dependencies": {
    "@cdktf/provider-archive": "^0.0.3",
    "@cdktf/provider-aws": "^2.0.9",
    "@cdktf/provider-local": "^0.0.3",
    "@cdktf/provider-null": "^0.2.3",
    "@cdktf/provider-pagerduty": "^0.0.3",
    "cdktf": "^0.5.0",
    "cdktf-cli": "^0.5.0",
    "constructs": "^3.3.147",
    "crypto-random-string": "^4.0.0",
    "parse-domain": "^4.1.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.1",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@types/chai": "^4.2.21",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.1",
    "chai": "^4.3.0",
    "constructs": "^3.3.147",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.0",
    "prettier": "^2.4.0",
    "semantic-release": "^17.4.7",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Pocket/terraform-modules.git"
  }
}
