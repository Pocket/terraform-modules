{
  "name": "@pocket/terraform-modules",
  "version": "0.0.0-development",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "license": "MPL-2.0",
  "scripts": {
    "get": "cdktf get",
    "build": "npm run get && tsc",
    "synth": "cdktf synth",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "mocha -r ts-node/register --exit \"src/**/*.spec.ts\"",
    "test-ci": "npm test -- --reporter mocha-junit-reporter --exit",
    "test:watch": "npm test -- --watch --watch-extensions ts -R min --watch-files src",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next",
    "lint-check": "eslint --fix-dry-run \"src/**/*.ts\"",
    "lint-fix": "eslint --fix \"src/**/*.ts\"",
    "semantic-release": "semantic-release"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "engines": {
    "node": ">=10.12"
  },
  "dependencies": {
    "cdktf": "0.0.16",
    "constructs": "^3.0.4",
    "crypto-random-string": "^3.2.0",
    "parse-domain": "^3.0.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "cdktf-cli": "0.0.16",
    "chai": "^4.2.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.1.1",
    "mocha-junit-reporter": "^2.0.0",
    "prettier": "^2.0.5",
    "semantic-release": "^17.1.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Pocket/terraform-modules.git"
  }
}
