{
  "name": "@pocket/terraform-modules",
  "version": "0.0.0-development",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "license": "MPL-2.0",
  "scripts": {
    "get": "cdktf get",
    "build": "rm -rf dist && npm run get && tsc",
    "build:dev": "npm run build && NODE_ENV=development npm run synth",
    "synth": "cdktf synth",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test-ci": "npm test -- --exit",
    "test:watch": "npm test -- --watch --watch-extensions ts -R min --watch-files src",
    "test": "jest",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next",
    "lint-check": "eslint --fix-dry-run \"src/**/*.ts\"",
    "lint-fix": "eslint --fix \"src/**/*.ts\"",
    "semantic-release": "semantic-release"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "engines": {
    "node": ">=10.12"
  },
  "peerDependencies": {
    "cdktf": "^0.2.2",
    "constructs": "^3.0.4"
  },
  "dependencies": {
    "crypto-random-string": "^4.0.0",
    "parse-domain": "^3.0.3"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@types/chai": "^4.2.21",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.3",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "cdktf": "0.2.2",
    "cdktf-cli": "^0.2.2",
    "chai": "^4.3.0",
    "constructs": "^3.3.107",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "semantic-release": "^17.4.4",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Pocket/terraform-modules.git"
  }
}
